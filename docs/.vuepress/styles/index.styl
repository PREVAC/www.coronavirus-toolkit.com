.vuepress-flowchart {
    overflow: hidden;
}

.vuepress-flowchart .1-element {
    display: none;
}

h1 {
    color: #990066;
}

h2, h3, h4, h5, h6 {
    color: #219a93;
}

.downloadtools {
    width: 200px;
}

.severe {
    color: #EB3225;
}

.moderate {
    color: #F08633;
}

.mild {
    color: #3D8268;
}